                    Grid.Row="1" 
                    VerticalAlignment="Stretch" 
                    HorizontalAlignment="Stretch" 
                    KeyDown="KeyStrokeHandler" 
                    Background="#D3BCC0" 
                    FontSize="{Binding fontSize}" 
                    FontWeight="Normal"
                    AcceptsTab="True" 
                    AcceptsReturn="True" 
                    Text="{Binding noteContent}" 
                    TextWrapping="Wrap" 
                    BorderThickness="0" 
                    BorderBrush="AliceBlue"
                    Foreground="White" 
                    CaretBrush="White" 
                    ScrollViewer.VerticalScrollBarVisibility="Hidden">
            <TextBox.Background>
                <SolidColorBrush Color="#121212" Opacity="0.5" />
            </TextBox.Background>
        </TextBox>

        <!-- bottom bar -->
        <Grid Grid.Row="2" Height="10" Focusable="True"
                ColumnDefinitions="* 13 13 13 13 13 13 13 13" RowDefinitions="10">
            <Rectangle Name="drag_rect_bottom" Grid.ColumnSpan="9" Height="10" HorizontalAlignment="Stretch" Fill="#71a049" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True"/>
            <PathIcon Name="bold_rect" Grid.Column="1" Height="10" Width="10" Data="{StaticResource text_bold_regular}" PointerPressed="OnPointerPressed" Focusable="True" Margin="3 0 0 0"/>
            <PathIcon Name="font_up" Grid.Column="2" Height="10" Width="10" Data="{StaticResource caret_up_regular}" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True" Margin="3 0 0 0"/>
            <PathIcon Name="font_down" Grid.Column="3" Height="10" Width="10" Data="{StaticResource caret_down_regular}" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True" Margin="3 0 0 0"/>
            <PathIcon Name="border_rect_l" Grid.Column="4" Height="10" Width="10" Data="{StaticResource table_move_left_regular}" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True" Margin="3 0 0 0"/>
            <PathIcon Name="border_rect_t" Grid.Column="5" Height="10" Width="10" Data="{StaticResource table_move_up_regular}" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True" Margin="3 0 0 0"/>
            <PathIcon Name="border_rect_r" Grid.Column="6" Height="10" Width="10" Data="{StaticResource table_move_right_regular}" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True" Margin="3 0 0 0"/>
            <PathIcon Name="border_rect_b" Grid.Column="7" Height="10" Width="10" Data="{StaticResource table_move_down_regular}" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True" Margin="3 0 0 0"/>
            <PathIcon Name="resize_rect" Grid.Column="8" Width="10" Height="10" Data="{StaticResource drag_regular}" PointerPressed="OnPointerPressed" PointerReleased="OnPointerReleased" Focusable="True" Margin="3 0 0 0"/>
        </Grid>
    </Grid>
    </Panel>

</UserControl>
